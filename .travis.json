{
  "os": "linux",
  "jdk": "oraclejdk8",
  "dist": "trusty",
  "cache": {
    "directories": [
      "$HOME/.ivy2/cache",
      "$HOME/.sbt"
    ]
  },
  "group": "stable",
  "scala": "2.11.12",
  "script": [
    "sbt clean test"
  ],
  "language": "scala",
  "before_cache": [
    "find $HOME/.ivy2 -name \"ivydata-*.properties\" -delete",
    "find $HOME/.sbt -name \"*.lock\" -delete"
  ]
}